  "main": "electron.js",
  "scripts": {
    "build": "rollup -c && electron-builder",
    "dev": "npm run format && echo 'Started Dev Enviroment' && rollup -c -w",
    "start": "echo 'Electron is Launching' && electron .",
    "validate": "svelte-check",
    "format-check": "prettier --check \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "format": "echo 'Started Formatting' && prettier --write \"{,!(node_modules|public)/**/}*.{js,svelte}\" && echo 'Ended Formatting'"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
