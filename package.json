    "@types/node": "^15.6.1",
    "autoprefixer": "^10.2.5",
    "electron-reload": "^1.5.0",
    "node-sass": "^6.0.0",
    "postcss": "^8.3.0",
    "prettier": "^2.3.0",
    "prettier-plugin-svelte": "^2.3.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.34.0",
    "sirv-cli": "^1.0.0",
    "svelte": "^3.0.0",
